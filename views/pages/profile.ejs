<%- include('../partials/header');%>
<nav class="navbar navbar-expand-md navbar-dark bg-dark">
    <a href="/dashboard" class="navbar-brand">Matcha</a>
    <a href="/logout" class="nav-link active">logout</a>
    <!-- <li class="nav-item"> -->
        <div class="dropdown">
          <a href="#" class="dropbtn">
            <!-- Dropdown<i class="fa fa-caret-down"></i> -->
            <!-- Notifications<i class="fa fa-bell" aria-hidden="true"></i> -->
            <i class="fa fa-bell" style="color: grey;" aria-hidden="true"></i>
          </a>
          <div class="dropdown-content">
            <!-- <a href="#">Link 1</a>
            <a href="#">Link 2</a>
            <a href="#">Link 3</a> -->
            <% if (getNotified !== undefined) { %>
              <% getNotified.forEach(function (result) { %>
                <a href="#"><%=result%></a>
              <% }) %>
            <% } %>
          </div>
        </div>
    <!-- </li> -->
</nav>
<a href="/user-profile">Edit settings</a>
<section>
    <img class="profileImage" src="<%=data.image%>" alt="profilePic">
    <h5>age:</h5> <%=data.age%><br>
    <h5>gender:</h5> <%=data.gender%><br>
    <h5>sexual Orientation:</h5> <%=data.sexualOrientation%><br>
    <h5>bio:</h5> <%=data.bio%><br>
    <h5>interests:</h5>
    <%if (interestValues !== undefined) {%>
        <ul>
            <% interestValues.forEach(function(result){ %>
                <li><%= result %></li>
            <%  }); %>
        </ul>
    <%}%>
</section>
