<% data.otherUser.forEach(function(user){ %>
    <% data.peopleNames.forEach(function(params){ %> 
      <% if (params.otherUserUsername === user.otherUsername) {%>
        <% data.otherImages.forEach(function(image) { %>
          <% if (params.otherUserUsername === image.username) {%>
            <!-- <div class="card"> -->
            <div>
              <img src="<%=image.imagePath%>" class="displayProfilePic" alt="Avatar" >
              <div class="container">
                <h4><b><%=params.otherFirstName%> <%=params.otherLastName%></b></h4>
                <p><%=user.otherAge%></p>
                <a href="/like/liketo=<%=params.otherUserUsername%>/likefrom=<%=data.username%>"><i class="fa fa-thumbs-up" aria-hidden="true"></i></a>
                <a href="/view/viewfrom=<%=data.username%>/viewto=<%=params.otherUserUsername%>">
                  <i class="fa fa-eye" aria-hidden="true"></i>
                </a>
                <a href="#">
                  <i class="fa fa-star" style="color:gold;" aria-hidden="true"></i>
                </a>
              </div>
            </div>
          <% } %>
        <% }); %>
      <% } %>
    <% }); %>
<% }); %>


